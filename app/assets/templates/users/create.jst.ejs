<h1>Sign Up</h1>



<form id="form_id" action="index.html" method="post">

  <label for="">
    Username
    <input type="text" name="user[username]" value="<%= user.escape('username') %>">
  </label>

  <label>
    Email
    <input type="text" name="user[email]" value="<%= user.escape("email") %>">
  </label>

  <label>
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <button name="button">Create Account</button>

</form>
