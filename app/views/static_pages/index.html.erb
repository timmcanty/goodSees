<div id="main-content">

</div>

<script charset="utf-8">
  <% if current_user %>
    GoodSees.currentUserFeaturedReel = <%= current_user.featured_id %>;
    GoodSees.currentUser = <%= current_user.id %>;
  <% end %>

  $(document).ready(function(){
    new GoodSees.Routers.Router({$rootEl : $('#main-content')});
    Backbone.history.start();
  });
</script>
